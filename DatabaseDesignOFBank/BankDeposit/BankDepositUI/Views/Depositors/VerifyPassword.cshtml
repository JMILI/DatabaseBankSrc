@{
    Layout = "_SignInLayout";

}

<form action="@Url.Action("Transfer","Depositors")">
    <div class="controls">
        <h3>向</h3>@Model.Cid<h3>账户转账:￥</h3>@Model.TtransferBalance</h3>
        <div class="form-group">
            <label for="colorpicker-rgb" class="col-sm-4 control-label">密码</label>
            <div class="col-sm-8">
                <input type="password" class="form-control colorpicker-element" id="colorpicker-rgb" name="Cpassword" value="">
            </div>
        </div>
        <div class="col-sm-offset-4 col-sm-8">
            <button type="submit" class="btn btn-greensea">提交</button>
            <button type="reset" class="btn btn-red">重新输入</button>
        </div>
    </div>
</form>